{
  "name": "audiens/adform-client",
  "description": "PHP client for adform API",
  "keywords": [
    "adform",
    "dmp",
    "client",
    "php"
  ],
  "type": "library",
  "homepage": "https://github.com/Audiens/adform-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Francesco Panina",
      "homepage": "https://github.com/ranpafin"
    },
    {
      "name": "Miloš Jovanović",
      "homepage": "https://github.com/toxonics"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-mbstring": "*",
    "ext-json": "*",

    "myclabs/php-enum": "~1.4",
    "league/oauth2-client": "^1.0",
    "predis/predis": "^1.0",
    "league/flysystem": "^1.0",
    "guzzlehttp/guzzle": "^6.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "vlucas/phpdotenv": "^2.5",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Audiens\\AdForm\\": "src/Audiens/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DMP\\Tests\\": "tests/dmp-client/"
    }
  }
}
