{
  "name": "audiens/adform-client",
  "description": "PHP client for adform API",
  "keywords": ["adform", "dmp", "client", "php"],
  "type": "library",
  "homepage": "https://github.com/ranpafin",
  "license": "MIT",
  "authors": [
    {
      "name": "Francesco Panina",
      "homepage": "https://github.com/ranpafin"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "guzzlehttp/guzzle": "~6.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "psr-4": {
      "DMP\\": "src/Audiens/dmp-client"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DMP\\Tests\\": "tests/dmp-client/"
    }
  }
}
